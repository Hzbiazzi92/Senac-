programa
{
	real meta
	real vendas
	cadeia nome
	
	funcao inicio()
	{
		escreva("Digite o nome do funcionário:")
		leia(nome)
		escreva("Digite a meta:")
		leia(meta)
		escreva("Quanto que você vendeu" + nome)
		leia(vendas)

		se (vendas < meta ){
			escreva("Você não ganhou")
		}
		senao {
			escreva("Você ganhou")
		}
	
		

	}
}
