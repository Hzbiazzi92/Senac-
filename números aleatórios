programa
{
	inclua biblioteca Util --> u
	inteiro cont, quant
	
	funcao inicio()
	{
		escreva("Para sair do aplicativo do programa tecle 0 \n")
		enquanto (verdadeiro) {
			escreva("Quantos n√∫meros deseja gerar? \n")
			leia(quant)
			se(quant == 0){
				pare
			}
			para(cont = 1; cont <= quant; cont++){
				escreva(u.sorteia(1,60) + " ")
			}
		}
	}
}
